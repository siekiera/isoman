<config>
  <locale name="pl_PL" defaultPattern="full">
    <pattern name="precise" value="dd.MM.yyyy, HH:mm:ss"/>
    <pattern name="full" value="dd.MM.yyyy, HH:mm"/>
    <pattern name="datetime" value="dd.MM.yyyy HH:mm"/>
    <pattern name="shorttime" value="HH:mm"/>
    <pattern name="shortdate" value="dd.MM.yyyy"/>
    <pattern name="year" value="yyyy"/>
    <pattern name="monthyear" value="MMMMM yyyy"/>
  </locale>
  <locale name="en_US" defaultPattern="full">
    <pattern name="precise" value="h:mm:ss a, d MMMM yyyy"/>
    <pattern name="full" value="h:mm a, d MMMM yyyy"/>
    <pattern name="datetime" value="dd.MM.yyyy HH:mm"/>
    <pattern name="shorttime" value="h:mm a"/>
    <pattern name="shortdate" value="MM/dd/yyyy"/>
    <pattern name="year" value="yyyy"/>
    <pattern name="monthyear" value="MMMMM yyyy"/>
  </locale>
</config>