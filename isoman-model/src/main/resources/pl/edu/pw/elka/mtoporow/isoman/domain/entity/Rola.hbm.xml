<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="pl.edu.pw.elka.mtoporow.isoman.domain.entity.Rola" table="role">

        <id name="id" type="java.lang.Long">
            <column name="id" not-null="true"/>
        </id>
        <property name="nazwa" type="java.lang.String">
            <column name="nazwa" not-null="true"/>
        </property>
        <property name="opis" type="java.lang.String">
            <column name="opis"/>
        </property>
        <set name="osoby" table="role_osob" inverse="true"
             lazy="true" fetch="select">
            <key>
                <column name="id_roli"/>
            </key>
            <many-to-many entity-name="pl.edu.pw.elka.mtoporow.isoman.domain.entity.Osoba">
                <column name="id_osoby"/>
            </many-to-many>
        </set>
    </class>
</hibernate-mapping>